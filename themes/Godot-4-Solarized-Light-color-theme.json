{
	"name": "Godot",
	"colors": {
		// Main Godot bg color #363d4a
		"activityBar.background": "#f7efdc",
		"banner.background": "#f7efdc",
		"titleBar.activeBackground": "#f7efdc",
		"titleBar.inactiveBackground": "#f7efdc",
		"tab.activeBackground": "#f7efdc",
		"panel.background": "#f7efdc",
		"list.hoverBackground": "#f7efdc",
		"dropdown.listBackground": "#f7efdc",
		//foregrounds fr above
		"activityBar.foreground": "#000000",
		"banner.foreground": "#000000",
		"titleBar.activeForeground": "#000000",
		"titleBar.inactiveForeground": "#000000",
		"tab.activeForeground": "#000000",
		"list.hoverForeground": "#000000",
		"dropdown.foreground": "#343434",
		// A litle darker version of the bg #292f39
		"sideBar.background": "#e3dbc9",
		"tab.inactiveBackground": "#e3dbc9",
		"editorGroupHeader.noTabsBackground": "#e3dbc9",
		"editorGroupHeader.tabsBackground": "#e3dbc9",
		"statusBar.background": "#e3dbc9",
		"input.background": "#e3dbc9",
		"dropdown.background": "#e3dbc9",
		"toolbar.hoverBackground": "#e3dbc9",
		"notifications.background": "#e3dbc9",
		"quickInput.background": "#e3dbc9",
		//foregrounds for above
		"sideBar.foreground": "#343434",
		"tab.inactiveForeground": "#5a5a5a",
		"statusBar.foreground": "#343434",
		"input.foreground": "#343434",
		"notifications.foreground": "#343434",
		"quickInput.foreground": "#343434",
		// Base Godot accent color #70bafa
		// Darker accent color #5993c5 
		"button.background": "#5993c5",
		"activityBarBadge.background": "#5993c5",
		// Even darker accent color #22272f
		"button.hoverBackground": "#4d7fab",
		// Debugging colors
		"statusBar.debuggingBackground": "#364359",
		// Other colors
		"list.inactiveSelectionBackground": "#fef6e2",
		"editorHoverWidget.background": "#fef6e2",
		"editor.background": "#fef6e2",
		"editor.foreground": "#000000",
		"sideBarTitle.foreground": "#000000",
		"editor.lineHighlightBackground": "#ffffff12",
		"editor.selectionHighlightBackground": "#ffffff12",
		"editorCursor.foreground": "#000000",
		// Godot error and warning colors
		"editorError.foreground": "#c82d1f",
		"editorWarning.foreground": "#d2ba18",
		"list.errorForeground": "#c82d1f",
		"errorForeground": "#c82d1f",
		"problemsErrorIcon.foreground": "#6b5553",
		"list.warningForeground": "#d2ba18",
		"problemsWarningIcon.foreground": "#d2ba18",
		// Bracket colorization
		"editorBracketHighlight.foreground1": "#2120a4",
		// "editorBracketHighlight.foreground2": "#9b8a68",
		// "editorBracketHighlight.foreground3": "#669664",
		// "editorBracketHighlight.foreground4": "#986598",
		// "editorBracketHighlight.foreground5": "#926264",
		// "editorBracketHighlight.foreground6": "#5d8d89",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				// "fontStyle": "italic",
				"foreground": "#14141480"
			}
		},
		{
			"name": "Variables, Text Color, Godot-Text Color",
			"scope": [
				"variable",
				"meta.function-call.arguments",
				"meta.indexed-name",
				"meta.attribute",
				"entity.name.variable",
			],
			"settings": {
				"foreground": "#393732"
			}
		},
		{
			"name": "Properties, Godot-Member Variable Color",
			"scope": [
				"variable.other.property",
				"variable.other.object.property"
			],
			"settings": {
				"foreground": "#0066ad"
			}
		},
		{
			"name": "Invalid, Godot-Breakpoint Color",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#ff786b"
			}
		},
		{
			"name": "Keyword, Storage, Godot-Keywords Color",
			"scope": [
				"keyword",
				"keyword.control",
				"storage.type",
				"storage.modifier",
				"constant.language",
				"support.constant",
				"keyword.other.unit",
				"keyword.other",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#e62282"
			}
		},
		{
			"name": "Godot-Number Color",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#008c47"
			}
		},
		{
			"name": "Operator, Misc, Godot-Symbol Color",
			"scope": [
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#00009c"
			}
		},
		{
			"name": "Functions, Member Color, Godot-Function Color",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"support.function.any-method"
			],
			"settings": {
				"foreground": "#0039e6"
			}
		},
		{
			"name": "String, Symbols, Markup Heading, Godot-String Color",
			"scope": [
				"string",
				"punctuation.definition.string",
				"punctuation.support.type.property-name",
				"support.type.property-name",
				"storage.type.string",
				"constant.other.symbol",
				"constant.other.key",
				"constant.character",
				"constant.escape",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#996b00"
			}
		},
		{
			"name": "Class, Godot-Base Type Color",
			"scope": [
				"entity.name",
				"entity.other.inherited-class",
				"keyword.type",
				"meta.function.gdscript",
				"entity.name.type.class",
				"entity.name.type.enum",
				"entity.name.type.interface",
				"meta.void"
			],
			"settings": {
				"foreground": "#009933"
			}
		},
		{
			"name": "Builtin Classes, Godot-Engine Type Color",
			"scope": [
				"entity.name.class.builtin",
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#1c8c66"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#78919b"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#1c8c66"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Comment keywords WARNING, TOOD/FIXME",
			"scope": [
				"comment.keywords.warnings"
			],
			"settings": {
				"foreground": "#b89c7a"
			}
		},
		{
			"name": "Comment keywords CRITICAL",
			"scope": [
				"comment.keywords.critical"
			],
			"settings": {
				"foreground": "#c45959"
			}
		},
		{
			"name": "Comment keywords NOTICE",
			"scope": [
				"comment.keywords.notices"
			],
			"settings": {
				"foreground": "#8fab82"
			}
		},
		{
			"name": "Keywords, Godot-Control Flow Keyword Color",
			"scope": [
				"keyword.control.flow",
			],
			"settings": {
				"foreground": "#bd1fcc"
			}
		},
		{
			"name": "Godot-Function Definition Color",
			"scope": [
				"entity.name.function.gdscript"
			],
			"settings": {
				"foreground": "#009999"
			}
		},
		{
			"name": "Godot-Annotation Color,  @ keyword",
			"scope": [
				"entity.name.function.decorator.gdscript",
			],
			"settings": {
				"foreground": "#cc5e00"
			}
		},
		{
			"name": "Loose constant, enum classname, class constants",
			"scope": [
				"constant.language.gdscript",
			],
			"settings": {
				"foreground": "#393732"
			}
		},
		{
			"name": "Godot-Global Function Color",
			"scope": [
				"meta.global-method",
				"meta.keyword.language.gdscript",
				"meta.function-global.gdscript"
			],
			"settings": {
				"foreground": "#5c2eb8"
			}
		},
		{
			"name": "Godot-Global Function Color",
			"scope": [
				"meta.literal.nodepath.gdscript constant.character.escape.gdscript",
				"meta.literal.nodepath.gdscript keyword.control.flow.gdscript",
				"extra.keyword.language.gdscript"
			],
			"settings": {
				"foreground": "#63c259"
			}
		},
		{
			"name": "Double comment color",
			"scope": [
				"comment.line.double-number-sign.gdscript"
			],
			"settings": {
				"foreground": "#7e94aa"
			}
		}
	]
}